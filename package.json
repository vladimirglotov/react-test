      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "react-dotenv": {
    "whitelist": [
      "API_URL"
    ]
  },
  "homepage": "http://vladimirglotov.github.io/react-test",
  "devDependencies": {
    "gh-pages": "^3.2.3"
